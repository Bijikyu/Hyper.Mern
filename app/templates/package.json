  "scripts": {
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:dev:db": "npm run stop:dev:db && mkdir db || echo \"db folder exists, skipping...\" && mongod --fork --dbpath db --logpath ~/log/mongodb.log",
    "stop:dev:db": "killall mongod || echo \"db not running, skipping...\"",
    "start:dev:server": "nodemon --watch server server/index.js",
    "start:dev": "cross-env NODE_ENV=developemt && npm run start:dev:db && npm run start:dev:server",
    "start:dev:all": "npm run start:dev:db && concurrently \"npm run wds\" \"npm run start:dev:server\"",
    "test": "mocha --reporter spec server/test/*.test.js --exit",
    "wds": "./node_modules/.bin/webpack-dev-server --open --hot",
    "docker": "docker-compose up",
    "build:dev": "./node_modules/.bin/webpack",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --mode=production",
    "lint": "npm run lintServer; npm run lintCSS",
    "lintCSS": "node_modules/.bin/stylelint --fix src/**/*",
    "lintServer": "node_modules/.bin/eslint --fix server/"
  }
